tknife log_level :info
log_location             STDOUT
node_name                '{{ chef_dk_config_node_name }}'
client_key               '{{ chef_dk_config_client_key }}'
validation_client_name   'chef-validator'
validation_key           '/etc/chef-server/chef-validator.pem'
chef_server_url          '{{ chef_dk_config_chef_server_url }}'
syntax_check_cache_path  '~/.chef/syntax_check_cache'
http_proxy '{{ chef_dk_config_http_proxy }}'
https_proxy '{{ chef_dk_config_https_proxy }}'
no_proxy '{{ chef_dk_config_no_proxy }}'
